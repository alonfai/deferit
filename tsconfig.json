{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "isolatedModules": true,
    "strictNullChecks": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["jest"]
  },
  "files": [
    "*.ts",
    "*.tsx"
  ],
  "include": ["**/*.ts|x"]
}
